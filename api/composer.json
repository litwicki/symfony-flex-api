{
  "name": "zoadilack/tavro-api",
  "description": "A small business dashboard application.",
  "license": "proprietary",
  "type": "project",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "app/AppKernel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7",
    "symfony/symfony": "3.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "symfony/swiftmailer-bundle": "^2.3",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/monolog-bundle": "^2.8",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "incenteev/composer-parameter-handler": "^2.0",
    "twig/twig": "^2.0",
    "jms/serializer": "^1.0.0",
    "jms/serializer-bundle": "^1.1",
    "ramsey/uuid": "^2.7",
    "aws/aws-sdk-php-symfony": "~1.0",
    "mjohnson/decoda": "6.*",
    "stof/doctrine-extensions-bundle": "~1.1@dev",
    "liip/imagine-bundle": "^1.3",
    "cocur/slugify": "^1.4",
    "knplabs/knp-gaufrette-bundle": "^0.3.0",
    "gregwar/captcha-bundle": "^2.0",
    "moontoast/math": "^1.1",
    "doctrine/doctrine-migrations-bundle": "^1.0",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "twig/extensions": "^1.4",
    "lexik/jwt-authentication-bundle": "^2.1",
    "zoadilack/tavro-core": "dev-master",
    "litwicki/common": "dev-master",
    "guzzlehttp/guzzle": "^6.0",
    "litwicki/chargify-bundle": "dev-master",
    "nelmio/cors-bundle": "^1.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Zoadilack/tavro-core.git"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "symfony/phpunit-bridge": "^2.7",
    "symfony/var-dumper": "^3.0",
    "fzaninotto/faker": "^1.6",
    "ddelnano/dredd-hooks-php": "^1.1"
  },
  "scripts": {
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative"
  }
}
