{
  "name": "zoadilack/tavro-core",
  "description": "The core backbone for TAVRO.",
  "type": "symfony-bundle",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Zoadilack, LLC",
      "email": "dev@zoadilack.com"
    }
  ],
  "keywords": [
    "Tavro Core Bundle"
  ],
  "license": [
    "proprietary"
  ],
  "require": {
    "php": ">=7",
    "litwicki/chargify-bundle": "dev-master",
    "litwicki/common": "dev-master",
    "symfony/symfony": "3.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "symfony/swiftmailer-bundle": "^2.3",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/monolog-bundle": "^2.8",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "incenteev/composer-parameter-handler": "^2.0",
    "twig/twig": "^2.0",
    "jms/serializer": "^1.0.0",
    "jms/serializer-bundle": "^1.1",
    "ramsey/uuid": "2.7.1",
    "aws/aws-sdk-php": "3.*",
    "mjohnson/decoda": "6.*",
    "stof/doctrine-extensions-bundle": "~1.1@dev",
    "liip/imagine-bundle": "^1.3",
    "cocur/slugify": "^1.4",
    "knplabs/knp-gaufrette-bundle": "^0.3.0",
    "gregwar/captcha-bundle": "^2.0",
    "moontoast/math": "^1.1",
    "doctrine/doctrine-migrations-bundle": "^1.0",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "twig/extensions": "^1.4",
    "lexik/jwt-authentication-bundle": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "symfony/phpunit-bridge": "^2.7",
    "symfony/var-dumper": "^3.0",
    "fzaninotto/faker": "^1.6",
    "ddelnano/dredd-hooks-php": "^1.1"
  },
  "autoload": {
    "psr-0": {
      "Tavro\\Bundle\\CoreBundle": ""
    }
  },
  "target-dir": "Tavro/Bundle/CoreBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "dev"
    }
  }
}
