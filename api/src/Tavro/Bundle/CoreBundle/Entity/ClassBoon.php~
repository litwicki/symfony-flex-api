<?php

namespace CamelotMods\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping AS ORM;

use JMS\Serializer\Annotation\Accessor;
use JMS\Serializer\Annotation\ExclusionPolicy;
use JMS\Serializer\Annotation\Expose;
use JMS\Serializer\Annotation\Type;
use JMS\Serializer\Annotation\VirtualProperty;
use Doctrine\ORM\Mapping\Table;

use CamelotMods\Bundle\CoreBundle\Model\Entity;
use CamelotMods\Bundle\CoreBundle\Model\EntityInterface;

/**
 * @ORM\Entity
 * @ORM\Table(name="camelot_class_boon", indexes={@ORM\Index(name="CLASS_BOON", columns={"class_id","boon_id"})})
 */
class ClassBoon extends Entity implements EntityInterface
{
    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\CamelotClass", inversedBy="class_boons")
     * @ORM\JoinColumn(name="class_id", referencedColumnName="id", nullable=false)
     */
    private $class;

    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\Boon", inversedBy="class_boons")
     * @ORM\JoinColumn(name="boon_id", referencedColumnName="id", nullable=false)
     */
    private $boon;

    /**
     * Set class
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\CamelotClass $class
     * @return ClassBoon
     */
    public function setClass(\CamelotMods\Bundle\CoreBundle\Entity\CamelotClass $class)
    {
        $this->class = $class;

        return $this;
    }

    /**
     * Get class
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\CamelotClass 
     */
    public function getClass()
    {
        return $this->class;
    }

    /**
     * Set boon
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\Boon $boon
     * @return ClassBoon
     */
    public function setBoon(\CamelotMods\Bundle\CoreBundle\Entity\Boon $boon)
    {
        $this->boon = $boon;

        return $this;
    }

    /**
     * Get boon
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\Boon 
     */
    public function getBoon()
    {
        return $this->boon;
    }
}
