<?php

namespace CamelotMods\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping AS ORM;

use JMS\Serializer\Annotation\Accessor;
use JMS\Serializer\Annotation\ExclusionPolicy;
use JMS\Serializer\Annotation\Expose;
use JMS\Serializer\Annotation\Type;
use JMS\Serializer\Annotation\VirtualProperty;
use Doctrine\ORM\Mapping\Table;

use CamelotMods\Bundle\CoreBundle\Model\Entity;
use CamelotMods\Bundle\CoreBundle\Model\EntityInterface;

/**
 * @ORM\Entity
 * @ORM\Table(name="camelot_race_boon", indexes={@ORM\Index(name="RACE_BOON", columns={"race_id","boon_id"})})
 */
class RaceBoon extends Entity implements EntityInterface
{
    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\Race", inversedBy="race_boons")
     * @ORM\JoinColumn(name="race_id", referencedColumnName="id", nullable=false)
     */
    private $race;

    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\Boon", inversedBy="race_boons")
     * @ORM\JoinColumn(name="boon_id", referencedColumnName="id", nullable=false)
     */
    private $boon;

    /**
     * Set race
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\CamelotRace $race
     * @return RaceBoon
     */
    public function setRace(\CamelotMods\Bundle\CoreBundle\Entity\Race $race)
    {
        $this->race = $race;

        return $this;
    }

    /**
     * Get race
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\CamelotRace 
     */
    public function getRace()
    {
        return $this->race;
    }

    /**
     * Set boon
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\Boon $boon
     * @return RaceBoon
     */
    public function setBoon(\CamelotMods\Bundle\CoreBundle\Entity\Boon $boon)
    {
        $this->boon = $boon;

        return $this;
    }

    /**
     * Get boon
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\Boon 
     */
    public function getBoon()
    {
        return $this->boon;
    }
}
