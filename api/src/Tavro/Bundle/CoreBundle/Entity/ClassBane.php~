<?php

namespace CamelotMods\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping AS ORM;

use JMS\Serializer\Annotation\Accessor;
use JMS\Serializer\Annotation\ExclusionPolicy;
use JMS\Serializer\Annotation\Expose;
use JMS\Serializer\Annotation\Type;
use JMS\Serializer\Annotation\VirtualProperty;
use Doctrine\ORM\Mapping\Table;

use CamelotMods\Bundle\CoreBundle\Model\Entity;
use CamelotMods\Bundle\CoreBundle\Model\EntityInterface;

/**
 * @ORM\Entity
 * @ORM\Table(name="camelot_class_bane", indexes={@ORM\Index(name="CLASS_BOON", columns={"class_id","bane_id"})})
 *
 */
class ClassBane extends Entity implements EntityInterface
{
    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\CamelotClass", inversedBy="class_banes")
     * @ORM\JoinColumn(name="class_id", referencedColumnName="id", nullable=false)
     */
    private $class;

    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\Bane", inversedBy="class_banes")
     * @ORM\JoinColumn(name="bane_id", referencedColumnName="id", nullable=false)
     */
    private $bane;

    /**
     * Set class
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\CamelotClass $class
     * @return ClassBane
     */
    public function setClass(\CamelotMods\Bundle\CoreBundle\Entity\CamelotClass $class)
    {
        $this->class = $class;

        return $this;
    }

    /**
     * Get class
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\CamelotClass 
     */
    public function getClass()
    {
        return $this->class;
    }

    /**
     * Set bane
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\Bane $bane
     * @return ClassBane
     */
    public function setBane(\CamelotMods\Bundle\CoreBundle\Entity\Bane $bane)
    {
        $this->bane = $bane;

        return $this;
    }

    /**
     * Get bane
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\Bane 
     */
    public function getBane()
    {
        return $this->bane;
    }
}
