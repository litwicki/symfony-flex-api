<?php

namespace CamelotMods\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping AS ORM;

use JMS\Serializer\Annotation\Accessor;
use JMS\Serializer\Annotation\ExclusionPolicy;
use JMS\Serializer\Annotation\Expose;
use JMS\Serializer\Annotation\Type;
use JMS\Serializer\Annotation\VirtualProperty;
use Doctrine\ORM\Mapping\Table;

use CamelotMods\Bundle\CoreBundle\Model\Entity;
use CamelotMods\Bundle\CoreBundle\Model\EntityInterface;

/**
 * @ORM\Entity
 * @ORM\Table(name="camelot_mod_image", indexes={@ORM\Index(name="MOD_COMMENT", columns={"image_id","mod_id"})})
 */
class ModImage extends Entity implements EntityInterface
{
    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\Image", inversedBy="mod_images")
     * @ORM\JoinColumn(name="image_id", referencedColumnName="id", nullable=false)
     */
    private $image;

    /**
     * @ORM\ManyToOne(targetEntity="CamelotMods\Bundle\CoreBundle\Entity\mod", inversedBy="mod_images")
     * @ORM\JoinColumn(name="mod_id", referencedColumnName="id", nullable=false)
     */
    private $mod;

    /**
     * Set image
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\Image $image
     * @return modImage
     */
    public function setImage(\CamelotMods\Bundle\CoreBundle\Entity\Image $image)
    {
        $this->image = $image;

        return $this;
    }

    /**
     * Get image
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\Image
     */
    public function getImage()
    {
        return $this->image;
    }

    /**
     * Set mod
     *
     * @param \CamelotMods\Bundle\CoreBundle\Entity\Mod $mod
     * @return modImage
     */
    public function setMod(\CamelotMods\Bundle\CoreBundle\Entity\Mod $mod)
    {
        $this->mod = $mod;

        return $this;
    }

    /**
     * Get mod
     *
     * @return \CamelotMods\Bundle\CoreBundle\Entity\Mod
     */
    public function getMod()
    {
        return $this->mod;
    }

}
